#include <iostream>
#include <conio.h>
#include <math.h>

using namespace std;

int main()
{
  double stopnie;
cout << "Podaj kat w stopniach: ";
cin >> stopnie;
double radiany =( stopnie * M_PI ) / 180.0f;
cout<< ""<<endl;
cout<<stopnie<<" Stopnie zamienione na radiany : "<<radiany<<endl;
    return 0;
}
